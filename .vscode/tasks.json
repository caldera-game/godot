{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"label": "debug build",
			"group": "build",
			"type": "shell",
			"command": "python3",
			"args": [
				"./scons-local/scons.py",
				"target=editor",
				"dev_build=yes",
			],
			"problemMatcher": "$msCompile"
		},
		{
			"label": "release build",
			"group": "build",
			"type": "shell",
			"command": "python3",
			"args": [
				"./scons-local/scons.py",
				"target=editor",
			],
			"problemMatcher": "$msCompile"
		}
	]
}