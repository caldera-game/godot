{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/*.o": true,
        "**/__pycache__": true,
    },
    "files.associations": {
        "*.proto": "c-proto",
        "*.gdextension": "toml",
        "*.j2": "jinja",
        "__split_buffer": "cpp",
        "deque": "cpp",
        "list": "cpp",
        "string": "cpp",
        "vector": "cpp",
        "*.inc": "cpp"
    }
}